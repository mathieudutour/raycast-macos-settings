{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "macos-settings-storage",
  "title": "macOS settings storage",
  "description": "Store and Restore macOS settings",
  "icon": "command-icon.png",
  "author": "mathieudutour",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Store settings",
      "subtitle": "macOS settings",
      "description": "Store macOS settings on GitHub",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/core": "^5.0.2",
    "@raycast/api": "^1.64.0",
    "@raycast/utils": "^1.10.1",
    "execa": "^8.0.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
